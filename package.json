{
  "name": "hasura-backend",
  "version": "0.0.1",
  "description": "This is backend for a todo application given as an internship task",
  "main": "dist/app.js",
  "scripts": {
    "build": "gulp",
    "coverage": "nyc --reporter=text mocha --no-deprecation --timeout 10000 dist/test/*.spec.js -x dist/test/*.spec.js",
    "dev": "nodemon --no-deprecation --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "dockerStart": "cd scripts && chmod 777 dockerCompose.sh && ./dockerCompose.sh && cd ..",
    "dockerStop": "docker-compose down",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "start": "gulp && cd dist/ && node --inspect=8990 --no-deprecation app.js",
    "test": "mocha --no-deprecation --timeout 10000 --require ts-node/register **/*.spec.ts"
  },
  "keywords": [
    "Hasura backend"
  ],
  "author": "Kautilya Tripathi <tripathi.kautilya@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "gulp": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/node": "^10.12.15",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "3.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.0",
    "winston": "2.4.0"
  }
}
